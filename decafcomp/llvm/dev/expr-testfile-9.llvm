  store i32 0, i32* %x
  store i32 10, i32* %x
  %x1 = load i32, i32* %x
  call void @print_int(i32 %x1)
  %x2 = load i32, i32* %x
  %AddTmp = add i32 %x2, 10
  store i32 %AddTmp, i32* %x
  %x3 = load i32, i32* %x
  call void @print_int(i32 %x3)
  ret i32 0
}
