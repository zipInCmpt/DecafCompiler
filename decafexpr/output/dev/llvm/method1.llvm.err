; ModuleID = 'Test'

declare void @print_int(i32)

define void @test(i32) {
entry:
  %a = alloca i32
  store i32 %0, i32* %a
  %x = alloca i32
  store i32 1, i32* %x
  call void @print_int(i32 1)
  %x1 = load i32, i32* %x
  call void @print_int(i32 %x1)
  ret void
}

define i32 @main() {
entry:
  %x = alloca i32
  store i32 1, i32* %x
  call void @print_int(i32 1)
  call void @test(i32 1)
  %x1 = load i32, i32* %x
  call void @print_int(i32 %x1)
  %x2 = load i32, i32* %x
  call void @test(i32 %x2)
  ret i32 0
}
